{% extends '::base.html.twig' %}

{% block body -%}
    <h1>BudgetGest list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nomenclature</th>
                <th>Ratio</th>
                <th>Assiette</th>
                <th>Budgetht</th>
                <th>Montanttva</th>
                <th>Budgetttc</th>
                <th>Poste</th>
                <th>Ouvrage</th>
                <th>Type</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('budgetgest_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.nomenclature }}</td>
                <td>{{ entity.ratio }}</td>
                <td>{{ entity.assiette }}</td>
                <td>{{ entity.budgetHt }}</td>
                <td>{{ entity.montantTva }}</td>
                <td>{{ entity.budgetTtc }}</td>
                <td>{{ entity.poste }}</td>
                <td>{{ entity.ouvrage }}</td>
                <td>{{ entity.type }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('budgetgest_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('budgetgest_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('budgetgest_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
